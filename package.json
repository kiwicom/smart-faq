{
  "name": "@kiwicom/smart-faq",
  "version": "0.0.1",
  "scripts": {
    "prepublish": "yarn build",
    "start": "NODE_ENV=development LANGUAGE=en webpack-dev-server --open --mode development",
    "build": "yarn clean && yarn build:lib",
    "build:bundle": "SENTRY_URL_STAGING=$SENTRY_URL_STAGING NODE_ENV=production LANGUAGE=en webpack --mode production",
    "build:pages": "yarn build:bundle && ./scripts/deploy-ghpages.sh",
    "build:lib": "babel --out-dir lib --ignore *.stories.js,*.test.js src",
    "build:flow": "flow-copy-source -v -i '**/*.?(stories|test).js' src lib",
    "clean": "rimraf lib es dist",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest --config=jest.json",
    "test-ci": "yarn flow:check && yarn eslint && yarn test && yarn translations:check",
    "cypress:open": "cypress open &",
    "cypress:run": "cypress run --browser chrome",
    "cypress:run-ci": "cypress run --record --key $CYPRESS_KEY",
    "flow": "flow --color always --show-all-errors --no-auto-start",
    "flow:check": "flow check",
    "flow:start": "flow start",
    "flow:status": "flow status",
    "flow:stop": "flow stop",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "deploy:surge": "yarn surge $PWD/dist/ https://smartfaq-$(git rev-parse --abbrev-ref HEAD).surge.sh",
    "deploy:updateURL": "yarn babel-node scripts/deploymentUtils.js updateLiveURL",
    "translations:collect": "i18next-scanner 'src/**/*.{js,jsx}'",
    "translations:check": "node scripts/checkTranslations.js",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "schema": "graphql get-schema dev"
  },
  "main": "lib/App.js",
  "author": "kiwi.com",
  "devDependencies": {
    "@octokit/rest": "^15.2.5",
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-knobs": "^3.3.15",
    "@storybook/addon-links": "^3.4.5",
    "@storybook/react": "4.0.0-alpha.7",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-relay": "1.5.0",
    "babel-plugin-transform-assets-import-to-string": "1.1.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "4.5.1",
    "cypress": "^2.1.0",
    "dotenv": "5.0.1",
    "dotenv-webpack": "^1.5.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-context-patch": "0.0.7",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.70.0",
    "flow-copy-source": "^1.3.0",
    "flow-typed": "^2.3.0",
    "fs-extra": "5.0.0",
    "glob-promise": "3.3.0",
    "graphql": "0.13.2",
    "graphql-cli": "2.15.8",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "3.0.6",
    "i18next-scanner": "2.4.6",
    "jest": "^22.4.2",
    "js-cookie": "2.2.0",
    "make-runnable": "^1.3.6",
    "minimist": "1.2.0",
    "mockdate": "^2.0.2",
    "prettier-eslint": "^8.8.1",
    "raven-js": "^3.25.2",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "relay-compiler": "1.5.0",
    "request": "2.83.0",
    "request-promise-native": "1.0.5",
    "rimraf": "^2.6.2",
    "storybook-router": "^0.3.3",
    "surge": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "4.1.0",
    "webpack-cli": "2.0.10",
    "webpack-dev-server": "3.1.0"
  },
  "dependencies": {
    "@kiwicom/orbit-components": "^0.0.0-rc7",
    "babel-runtime": "^6.26.0",
    "i18next": "10.5.0",
    "idx": "2.2.0",
    "isomorphic-fetch": "2.2.1",
    "luxon": "1.2.0",
    "markdown-it": "^8.4.1",
    "react-i18next": "7.4.0",
    "react-relay": "1.5.0",
    "react-responsive": "^4.1.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "relay-runtime": "1.5.0",
    "styled-jsx": "^2.2.5"
  },
  "peerDependencies": {
    "react": "16.3.2",
    "react-dom": "16.3.2"
  }
}
